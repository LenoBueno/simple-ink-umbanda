{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react",
    "outDir": "./dist",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": true,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": false,
    "noUnusedLocals": false,
    "strictNullChecks": true
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts"
  ],
  "references": []
}
